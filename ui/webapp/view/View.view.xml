<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="sap.ui.demo.ui.controller.View" displayBlock="true">
	<Page title="{i18n>title}">
		<content>
			<Toolbar>
				<Input value="{/item}"/><Button text="add item" press="onAddItem"/></Toolbar>
				<List items="{/items}">
					<StandardListItem title="{item}"></StandardListItem>
				</List>
		</content>
	</Page>
</mvc:View>